{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gshell",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pipeline"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "variable_assignment"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "command": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "command_name"
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "FIELD",
              "name": "argument",
              "content": {
                "type": "SYMBOL",
                "name": "_argument"
              }
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "redirection"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "command_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "builtin_command"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        }
      ]
    },
    "builtin_command": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "cd"
          },
          {
            "type": "STRING",
            "value": "echo"
          },
          {
            "type": "STRING",
            "value": "alias"
          },
          {
            "type": "STRING",
            "value": "exit"
          },
          {
            "type": "STRING",
            "value": "export"
          },
          {
            "type": "STRING",
            "value": "source"
          },
          {
            "type": "STRING",
            "value": "set"
          },
          {
            "type": "STRING",
            "value": "unset"
          },
          {
            "type": "STRING",
            "value": "history"
          },
          {
            "type": "STRING",
            "value": "type"
          },
          {
            "type": "STRING",
            "value": "net-test"
          },
          {
            "type": "STRING",
            "value": "net-resolve"
          },
          {
            "type": "STRING",
            "value": "net-fetch"
          },
          {
            "type": "STRING",
            "value": "net-scan"
          },
          {
            "type": "STRING",
            "value": "use_starship"
          },
          {
            "type": "STRING",
            "value": "enable_plugin"
          },
          {
            "type": "STRING",
            "value": "load_vivid_theme"
          },
          {
            "type": "STRING",
            "value": "setenv"
          },
          {
            "type": "STRING",
            "value": "getenv"
          },
          {
            "type": "STRING",
            "value": "command_exists"
          },
          {
            "type": "STRING",
            "value": "path_exists"
          },
          {
            "type": "STRING",
            "value": "exec"
          }
        ]
      }
    },
    "_argument": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "raw_string"
        },
        {
          "type": "SYMBOL",
          "name": "expansion"
        },
        {
          "type": "SYMBOL",
          "name": "command_substitution"
        },
        {
          "type": "SYMBOL",
          "name": "flag"
        }
      ]
    },
    "flag": {
      "type": "PATTERN",
      "value": "--?[a-zA-Z0-9_-]+"
    },
    "word": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_.\\/-]+"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_content"
              },
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "SYMBOL",
                "name": "expansion"
              },
              {
                "type": "SYMBOL",
                "name": "command_substitution"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "raw_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "raw_string_content"
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "string_content": {
      "type": "PATTERN",
      "value": "[^\"$\\\\]+"
    },
    "raw_string_content": {
      "type": "PATTERN",
      "value": "[^']+"
    },
    "escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\."
    },
    "expansion": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "$"
            },
            {
              "type": "SYMBOL",
              "name": "variable_name"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "${"
            },
            {
              "type": "SYMBOL",
              "name": "variable_name"
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "$"
            },
            {
              "type": "PATTERN",
              "value": "[0-9]+"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "$"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "@"
                },
                {
                  "type": "STRING",
                  "value": "*"
                },
                {
                  "type": "STRING",
                  "value": "?"
                },
                {
                  "type": "STRING",
                  "value": "$"
                },
                {
                  "type": "STRING",
                  "value": "!"
                },
                {
                  "type": "STRING",
                  "value": "-"
                }
              ]
            }
          ]
        }
      ]
    },
    "variable_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "command_substitution": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "$("
            },
            {
              "type": "SYMBOL",
              "name": "command"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "`"
            },
            {
              "type": "SYMBOL",
              "name": "command"
            },
            {
              "type": "STRING",
              "value": "`"
            }
          ]
        }
      ]
    },
    "variable_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "variable_name"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "word"
              },
              {
                "type": "SYMBOL",
                "name": "string"
              },
              {
                "type": "SYMBOL",
                "name": "raw_string"
              },
              {
                "type": "SYMBOL",
                "name": "expansion"
              },
              {
                "type": "SYMBOL",
                "name": "command_substitution"
              }
            ]
          }
        }
      ]
    },
    "pipeline": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "command"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "operator",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "|"
                      },
                      {
                        "type": "STRING",
                        "value": "&&"
                      },
                      {
                        "type": "STRING",
                        "value": "||"
                      }
                    ]
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "command"
                }
              ]
            }
          }
        ]
      }
    },
    "redirection": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": ">"
                },
                {
                  "type": "STRING",
                  "value": ">>"
                },
                {
                  "type": "STRING",
                  "value": "<"
                },
                {
                  "type": "STRING",
                  "value": "2>"
                },
                {
                  "type": "STRING",
                  "value": "2>>"
                },
                {
                  "type": "STRING",
                  "value": "&>"
                },
                {
                  "type": "STRING",
                  "value": "&>>"
                }
              ]
            }
          },
          {
            "type": "FIELD",
            "name": "target",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "word"
                },
                {
                  "type": "SYMBOL",
                  "name": "string"
                },
                {
                  "type": "SYMBOL",
                  "name": "expansion"
                }
              ]
            }
          }
        ]
      }
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [
    [
      "command"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}